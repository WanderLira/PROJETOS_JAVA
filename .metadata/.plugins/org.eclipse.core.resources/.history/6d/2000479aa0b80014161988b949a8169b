package testacarro;


public class TestaCarro {

	public static void main(String[] args) {
		
		Carro carro = new Carro();
		carro.setCor("Azul");
		carro.setModelo("Ford Fiesta");
		carro.setVelocidadeAtual(0);
		carro.setVelocidadeMaxima(120);
		
		carro.Liga();
		
		carro.Acelera(20);
		System.out.println(carro.getVelocidadeAtual());
		
